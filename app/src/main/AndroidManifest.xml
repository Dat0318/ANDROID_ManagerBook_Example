<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.practiceapplication">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PracticeApplication">
        <activity android:name=".WebsiteActivity"/>
        <activity android:name=".FavoriteBook"
            android:label="@string/activity_favorites_books"/>
        <activity android:name=".CurrentReadBook"
            android:label="@string/activity_currently_reading" />
        <activity android:name=".WantToReadActivity"
            android:label="@string/activity_want_to_books" />
        <activity android:name=".AlreadyReadBookActivity"
            android:label="@string/activity_already_read" />
        <activity android:name=".BookActivity" />
        <activity android:name=".SecondActivity"
            android:label="@string/activity_all_books"/>
        <activity android:name=".MainActivity" />
        <activity android:name=".ProfileActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>